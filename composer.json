{
    "name": "elyerr/api-response",
    "description": "Proporciona funcionalidad extra a una API bajo el framework Laravel",
    "type": "library",
    "license": "AGPL-3.0-only",
    "extra": {
        "laravel": {
            "providers": [
                "Elyerr\\ApiResponse\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Elyerr\\ApiResponse\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Elvis  Yerel Roman Concha",
            "email": "yerel9212@yahoo.es"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "php": "^8.0.2",
        "laravel/framework": "^9.0", 
        "spatie/laravel-fractal": "^6.0",
        "phpunit/phpunit": "^10.4"
    }
}
